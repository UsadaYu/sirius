prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: @SIRIUS_TARGET_NAME@
Description: sirius
Version: @SIRIUS_VERSION@
URL: https://github.com/UsadaYu/sirius
Libs: -L${libdir} @_pkg_interface_link_dir@ @_pkg_interface_link_libraries@
Libs.private: @_pkg_interface_private_link_dir@ @_pkg_interface_private_link_libraries@
Cflags: -I${includedir}
